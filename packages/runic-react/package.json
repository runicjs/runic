    "src"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "build": "pnpm clean && tsc",
    "prepublishOnly": "pnpm build",
    "test": "vitest --run",
    "test:watch": "vitest"
  },
  "devDependencies": {
    "@testing-library/dom": "^10.4.0",
    "@testing-library/react": "^16.2.0",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@vitejs/plugin-react": "^4.3.4",
    "jsdom": "^26.0.0",
    "rimraf": "^6.0.1",
    "typescript": "~5.6.2",
    "vitest": "^3.0.4"
  },
  "dependencies": {
    "@runicjs/runic": "workspace:*",
    "react": "^18.3.1"
  }
